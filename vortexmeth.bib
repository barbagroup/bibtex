	Journal = {J. Comp.\ Phys.},
	Keywords = {remeshing, aircraft trailing vortices},
	Number = {13--16},
	Pages = {1296--1304},
	Title = {Billion vortex particle direct numerical simulations of aircraft wakes},
	Volume = {197},
	Year = {2008}}

